<Window x:Class="Casa.path"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Casa"
        mc:Ignorable="d"
        Title="path" Height="450" Width="800">
    <Canvas>
        <Path Stroke="Black" StrokeThickness="2"
              Data=
              "M5,5 105,5 105,105 5, 105 Z
               M125,5 225,5 "
              ></Path>
        <Path StrokeThickness="2" Stroke="Black" Fill="Red" 
              Data="F1 M550,200 L600,300 500,300 Z M525,225 H575 V325 H525 Z"
              ></Path>
        <Path StrokeThickness="2" Stroke="Black" Fill="Red"  Canvas.Left="-150"
              Data="F0 M550,200 L600,300 500,300 Z M525,225 H575 V325 H525 Z"
              ></Path>
        <Path StrokeThickness="5" Stroke="Black" Fill="Yellow"  Canvas.Left="-350"
              Data="F1 M550,200 L600,300 500,300 Z M525,225 H575 v325 H525 Z"
              ></Path>
    </Canvas>
</Window>
